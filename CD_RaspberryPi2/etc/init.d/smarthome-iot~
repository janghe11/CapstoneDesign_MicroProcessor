#! /bin/sh

# Smarthome IoT Tesh Bench for Raspberry Pi

case "$1" in
    status)
# contents area
        ;;

    start|\
    stop|\
    restart|\
    force-reload)
        $TLP init $1
        ;;

     *)
        echo "Usage: $0 start|stop|restart|force-reload|status" 1>&2
        exit 3
        ;;
esac

exit 0
